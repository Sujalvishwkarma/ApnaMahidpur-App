{
  "expo": {
    "name": "apnamahidpur",
    "slug": "apnamahidpur",
    "scheme": "apnamahidpur",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/company-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/company-logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "WRITE_CONTACTS"
      ],
      "package": "com.sujalvishwkarma.apnamahidpur"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow Apna Mahidpur to access your contacts."
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "421d47ce-6c13-427e-9ac7-752e7123cfd7"
      }
    }
  }
}
